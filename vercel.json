{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/enterprise",
      "dest": "/enterprise.html"
    },
    {
      "src": "/white-label",
      "dest": "/white-label.html"
    }
  ]
}
